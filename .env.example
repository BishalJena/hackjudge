# HackJudge AI - Environment Variables

# GitHub OAuth (required for GitHub authentication)
GITHUB_CLIENT_ID=xxxxx
GITHUB_CLIENT_SECRET=xxxxxxx
GITHUB_REDIRECT_URI=http://localhost:3000/api/auth/github/callback

# App URL
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Kestra Integration (for production)
KESTRA_API_URL=http://localhost:8080/api/v1
KESTRA_NAMESPACE=hackjudge
KESTRA_USERNAME=admin@kestra.io
KESTRA_PASSWORD=Admin1234

# Together AI (for LLM agents - primary)
TOGETHER_API_KEY=your_together_ai_key

# OpenRouter (fallback LLM provider)
OPENROUTER_API_KEY=your_openrouter_key

# OpenAI (fallback LLM provider)
OPENAI_API_KEY=your_openai_key

# Database (for production)
DATABASE_URL=postgresql://user:password@localhost:5432/hackjudge

# Storage (for artifacts)
S3_BUCKET=hackjudge-artifacts
S3_ACCESS_KEY=your_s3_access_key
S3_SECRET_KEY=your_s3_secret_key
S3_REGION=us-east-1

# Firecrawl (fast web scraping for hackathon pages)
FIRECRAWL_API_KEY=fc-xxx

# Exa AI (web search for improved suggestions)
EXA_API_KEY=xxxx
